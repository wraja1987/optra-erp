{
  "name": "mobile",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "expo start --clear",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "test": "jest",
    "expo:doctor": "expo doctor",
    "expo:export:test": "expo export --platform web --force --output-dir tmp/expo-export"
  },
  "dependencies": {
    "expo": "^51.0.0",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.26",
    "expo-linking": "^5.0.2",
    "expo-notifications": "^0.28.11",
    "expo-secure-store": "^13.0.2"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-native": "^0.73.0",
    "eslint": "^9.7.0",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": []
  }
}



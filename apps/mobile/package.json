{
  "name": "mobile",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "expo start --clear",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "test": "jest",
    "expo:doctor": "expo doctor",
    "expo:export:test": "expo export --platform web --force --output-dir tmp/expo-export",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "expo": "^52.0.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.26",
    "expo-linking": "~6.3.1",
    "expo-notifications": "^0.28.11",
    "expo-secure-store": "^13.0.2",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.79",
    "eslint": "^9.7.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": []
  },
  "version": "1.0.0"
}

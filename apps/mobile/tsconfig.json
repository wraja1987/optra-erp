{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "noEmit": true,
    "skipLibCheck": true,
    "types": ["jest", "react", "react-native"]
  },
  "include": ["**/*.ts", "**/*.tsx"]
}



